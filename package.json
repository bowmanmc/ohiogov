{
  "name": "ohiorepresents",
  "version": "1.0.0",
  "description": "Ohio elected representatives",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "presass": "sass-lint -c .sass-lint.yml -v",
    "sass": "node-sass --source-map true --include-path src/sass --include-path node_modules src/sass/main.scss src/styles/bundle.css",
    "postsass": "npm-run-all --parallel postcss",
    "sass:watch": "nodemon -e scss -w src/sass -x \"npm run sass\"",
    "postcss": "postcss -c postcss.config.json",
    "prestart": "npm-run-all start-message lint",
    "start": "npm-run-all --parallel open:src watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "watch": "npm-run-all --parallel sass:watch lint:watch"
  },
  "author": "Applied Information Sciences",
  "dependencies": {
